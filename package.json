{
  "name": "redux-devtools-preset",
  "version": "0.0.4",
  "description": "Opinionated Redux DevTools",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib dist coverage",
    "test": "csc",
    "prepublish": "npm-run-all clean build"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "fbjs": "^0.8.4",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-slider-monitor": "^1.0.7"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "csc": "0.1.0",
    "npm-run-all": "^3.0.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "rimraf": "^2.5.4"
  },
  "repository": "ratson/redux-devtools-preset",
  "license": "MIT",
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "src"
  ],
  "xo": {
    "env": ["browser"],
    "rules": {
      "react/jsx-filename-extension": "off"
    }
  }
}
